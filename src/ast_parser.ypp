         $$ = new ASTConstantDeclaration(mods, name, $5);
         $$->loc.firstLine = @1.firstLine;
         $$->loc.firstCol = @1.firstCol;
         $$->loc.lastLine = @6.firstLine;
         $$->loc.lastCol = @6.firstCol;
