 // destructor actions for elements that need deleting when parse errors occur
%destructor { delete $$; } DOT_KW_IDENTIFIER QUOTED_WORD REGEX REGEX_EXTRACT REGEX_SUBST REGEX_TRANS alt_hash base_constructor base_constructor_list block case_block case_code classvardecl class_def context_mod exp exp_c exp_n gvardecl hash hash_element inline_methoddef internal_member_list list list_n member member2 myexp namespace_decl optname outofline_methoddef private_member_list public_member_list qtypedef return_statement return_value scalar scoped_const_decl scoped_sub_def statement statements statement_or_block string sub_def superclass switch_statement top_level_command top_namespace_decl try_statement uncqtypedef unscoped_const_decl
%destructor { free($$); } BACKQUOTE BASE_CLASS_CALL BINARY CLASS_SCOPED_REF CLASS_STRING COMPLEX_CONTEXT_REF CONTEXT_REF DATETIME IDENTIFIER KW_IDENTIFIER_OPENPAREN NUMBER REQUIRES QORE_CAST SCOPED_REF SCOPED_VREF SELF_REF VAR_REF ident_openparen
%destructor { if ($$) { for (int i = 0, count = $$->size(); i < count; i++) {delete (*$$)[i];} delete $$; } } base_constructors class_attributes context_mods inheritance_list member_list member_list2 namespace_decls superclass_list

