         $$ = new ASTStatementBlock;
         $$->loc.firstLine = @1.firstLine;
         $$->loc.firstCol = @1.firstCol;
         $$->loc.lastLine = @1.lastLine;
         $$->loc.lastCol = @1.lastCol;
