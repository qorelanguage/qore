      ;

scoped_const_decl:
      TOK_CONST SCOPED_REF '=' exp ';' {
         ASTName name($2, ASTNameKind::ANK_ScopedRef);
         name.loc.firstLine = @2.firstLine;
         name.loc.firstCol = @2.firstCol;
