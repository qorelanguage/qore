string:
      QUOTED_WORD {
         $$ = $1;
      }
      | QUOTED_WORD string {
         $$ = $1;
         $$->append(*($2));
         delete $2;
      }
      ;

scalar:
      QFLOAT        { $$ = new ASTLiteralExpression(ALEKind::ALEK_Float, $1); }
      | INTEGER     { $$ = new ASTLiteralExpression(ALEKind::ALEK_Int, $1); }
      | string      { $$ = new ASTLiteralExpression(ALEKind::ALEK_String, $1); }
