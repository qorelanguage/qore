class QueuedObserver inherits DataProvider::Observer {
    private {
        Queue q();
    }

    Queue getQueue() {
        return q;
    }

    #! This method is called when an event is raised
    update(string event_id, hash<auto> data_) {
        q.push(data_."data");
    }
}
