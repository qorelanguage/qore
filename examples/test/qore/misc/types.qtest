    issue2791() {
        {
            Test t();
            t.a = {"a": "two"};
            hash h = t{"a",};
            assertEq(Type::Hash, h.a.fullType());
        }
        {
            hash<auto> h1 = {
                "a": {"a": "a"},
            };
            hash h = h1{"a",};
            assertEq(Type::Hash, h.a.fullType());
        }
        {
            hash h;
            h.a = hash<MyHash>{};
            # ensure types are stripped on assignment with internal values of untyped hashes
            assertEq(Type::Hash, h.a.fullType());
        }
        {
            hash h = {
                "a": {"a": 1},
                "b": 1,
            };
            h.a = hash<MyHash>{};
            # ensure types are stripped on assignment with internal values of untyped hashes
            # also with type folding
            assertEq(Type::Hash, h.a.fullType());
        }
        {
            auto h = {
                "a": {"a": 1},
                "b": {"a": "1"},
            };
            # ensure consistent type folding
            assertEq("hash<string, hash<auto>>", h.fullType());
            assertEq("hash<string, int>", h.a.fullType());
        }
        {
            auto h = {
                "a": {"a": 1},
            } + {
                "b": {"a": "1"},
            };
            # ensure consistent type folding
            assertEq("hash<string, hash<auto>>", h.fullType());
            assertEq("hash<string, int>", h.a.fullType());
            h.a.b = 2;
        }
        {
            any h;
            h.a = hash<MyHash>{};
            # ensure types are stripped on assignment with internal values of untyped hashes
            assertEq(Type::Hash, h.a.fullType());
        }
        {
            hash<auto> h;
            h.a = hash<MyHash>{};
            assertEq("hash<MyHash>", h.a.fullType());
        }
        {
            auto h = {"a": {"a": 2}} + {"b": {"a": "1"}};
            assertEq("hash<string, int>", h.a.fullType());
            assertEq("hash<string, string>", h.b.fullType());
            assertEq("hash<string, hash<auto>>", h.fullType());
        }
        {
            list<auto> l = ({"a": "a"}, );
            list l1 = l[0..];
            assertEq("list<hash<string, string>>", l.fullType());
            assertEq("hash<string, string>", l[0].fullType());
            assertEq(Type::Hash, l1[0].fullType());
            l1 = extract l, 0;
            assertEq(Type::Hash, l1[0].fullType());
        }
        {
            list l;
            l += hash<MyHash>{};
            # ensure types are stripped on assignment with internal values of untyped lists
            assertEq(Type::Hash, l[0].fullType());
        }
        {
            list l = ({"a": 1}, 1);
            # ensure types are stripped on assignment with internal values of untyped hashes
            # also with type folding
            assertEq(Type::Hash, l[0].fullType());
        }
        {
            any l;
            l[0] = hash<MyHash>{};
            # ensure types are stripped on assignment with internal values of untyped lists
            assertEq(Type::Hash, l[0].fullType());
        }
        {
            list<auto> l;
            l += hash<MyHash>{};
            assertEq("hash<MyHash>", l[0].fullType());
        }
        {
            list l;
            l += hash<MyHash>{};
            # ensure types are stripped on assignment with internal values of untyped lists
            assertEq(Type::Hash, l[0].fullType());
        }
        {
            auto l = (1, 2,) + ("3",);
            assertEq("list<auto>", l.fullType());
        }
        {
            auto l = (1, 2,) + "3";
            assertEq("list<auto>", l.fullType());
        }
        {
            auto l = ("3",) + (1, 2,);
            assertEq("list<auto>", l.fullType());
        }
        {
            auto l = "3" + (1, 2,);
            assertEq("list<auto>", l.fullType());
        }
    }

