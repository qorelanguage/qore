#!/usr/bin/env qore
# -*- mode: qore; indent-tabs-mode: nil -*-

%new-style
%enable-all-warnings
%require-types
%strict-args
%no-child-restrictions

# we will test invalid operations
%disable-warning invalid-operation

%requires ../../../../qlib/QUnit.qm

%exec-class TypeTest

class Test {
    public {
        auto a;
    }
}

public hashdecl MyHash {
    int x = 1;
}

const Test = (
    "a": {
        "a": 1,
    },
);

int sub rt(reference<list<string>> r) {
    return 1;
}

const TestList = (1, 2, 3);

class Issue3887_Base { abstract int test(softlist<auto> l); }
class Issue3887_Child inherits Issue3887_Base { int test(softlist l) { return 1; } }

class TypeTest inherits QUnit::Test {
    constructor() : QUnit::Test("TypeTest", "1.0") {
        addTestCase("-= test", \minusEqualsTest());
        addTestCase("issue 4133 complex types", \issue4133());
        addTestCase("issue 3887", \issue3887());
        addTestCase("hashdecl types", \hashdeclTypeTest());
        addTestCase("<string>::toBinary()", \stringToBinary());
        addTestCase("issue 3448", \issue3448());
        addTestCase("issue 3438", \issue3438());
        addTestCase("issue 3429", \issue3429());
        addTestCase("issue 3404", \issue3404());
        addTestCase("issue 3278", \issue3278());
        addTestCase("issue 3210", \issue3210());
        addTestCase("issue 3157", \issue3157());
        addTestCase("issue 2980", \issue2980());
        addTestCase("issue 2877", \issue2877());
        addTestCase("issue 2869", \issue2869());
        addTestCase("issue 2791", \issue2791());
        addTestCase("issue 2767", \issue2767());
        addTestCase("complex assignments", \testComplexAssignments());
        addTestCase("auto", \testComplexAutoTypes());
        addTestCase("complex type compat", \testComplexTypesCompat());
        addTestCase("types", \testTypes());
        addTestCase("issue 2651", \issue2651());
        addTestCase("issue 2647", \issue2647());

        set_return_value(main());
    }

    minusEqualsTest() {
        # issue #4873
        {
            *hash<string, bool> h;
            h -= (1,);
            assertNothing(h);
        }
        {
            *hash<string, bool> h = {};
            h -= (1,);
            assertEq({}, h);
        }
        {
            hash<string, bool> h;
            h -= (1,);
            assertNothing(h);
        }
        {
            hash<string, bool> h = {};
            h -= (1,);
            assertEq({}, h);
        }
    }

    issue4133() {
        {
           *hash<string, softlist<auto>> h = {"a": 1};
           assertEq((1,), h.a);
        }
        {
           *hash<string, softlist<auto>> h += {"a": 1};
           assertEq((1,), h.a);
        }
        {
           *hash<string, softlist<auto>> h;
           map h = {"a": 1}, 1;
           assertEq((1,), h.a);
        }
        {
           *hash<string, softlist<auto>> h;
           map h += {"a": 1}, 1;
           assertEq((1,), h.a);
        }
    }

    issue3887() {
        assertEq(1, (new Issue3887_Child()).test());
    }

    hashdeclTypeTest() {
        # issue #3882
        hash<DateTimeInfo> h = now().info(){"month", "day"};
        assertEq("hash<DateTimeInfo>", h.fullType());

        hash<DateTimeInfo> h0 = h + now().info(){"month",};
        assertEq("hash<DateTimeInfo>", h0.fullType());
    }

    stringToBinary() {
        assertEq(<68656c6c6f>, "hello".toBinary());
    }

    issue3448() {
        {
            Program p(PO_NEW_STYLE);
            p.parse("softlist<auto> l0 = (1,); list<int> l1 = l0; remove l1;", "issue-3448");
        }
        {
            Program p(PO_NEW_STYLE);
