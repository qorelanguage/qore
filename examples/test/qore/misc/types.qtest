    issue3429() {
        hash<auto> h = {
            "a": <StatInfo>{"size": 1},
            "b": {"a": "string"},
            "c": {"a": 1},
        };

        assertEq("hash<StatInfo>", h.a.fullType());
        assertEq("hash<auto>", ({"a": 1} + {"b": "2"}).fullType());
        assertEq("list<auto>", ((1,) + ("2",)).fullType());
        assertEq("list<auto>", ((1,) + "2").fullType());
        assertEq("list<auto>", (1 + ("2",)).fullType());

        assertEq("hash<string, *int>", ({"a": 1} + {"b": NOTHING}).fullType());
        assertEq("list<*int>", ((1,) + (NOTHING,)).fullType());

        assertEq("hash<auto>", (<StatInfo>{} + {"a": 1}).fullType());
        assertEq("hash<auto>", ({"a": 1} + <StatInfo>{}).fullType());

        # test the hash map operator
        assertEq("hash<string, hash<StatInfo>>", (map {$1.key: $1.value}, {"a": <StatInfo>{}}.pairIterator()).fullType());
        assertEq("hash<string, hash<StatInfo>>", (map {$1.key: $1.value}, {"a": <StatInfo>{}}.pairIterator(), True).fullType());
        assertEq("hash<string, hash<StatInfo>>", (map {$#.toString(): $1}, <StatInfo>{}).fullType());
        assertEq("hash<string, hash<StatInfo>>", (map {$#.toString(): $1}, (<StatInfo>{},)).fullType());
        assertEq("hash<string, hash<StatInfo>>", (map {$#.toString(): $1}, <StatInfo>{}, True).fullType());
        assertEq("hash<string, hash<StatInfo>>", (map {$#.toString(): $1}, (<StatInfo>{},), True).fullType());

        assertEq("hash<StatInfo>", (map {$1.key: $1.value}, {"a": <StatInfo>{}}.pairIterator()).a.fullType());
        assertEq("hash<StatInfo>", (map {$1.key: $1.value}, {"a": <StatInfo>{}}.pairIterator(), True).a.fullType());

        {
            hash h0;
            h0.a.b = <StatInfo>{};
            assertEq("hash", h0.a.b.fullType());
        }

        {
            list<hash<StatInfo>> l = map $1, (<StatInfo>{},);
            assertEq("hash<StatInfo>", l[0].fullType());
            assertEq("list<hash<StatInfo>>", l.fullType());
        }

        {
            list<hash<StatInfo>> l = map $1, (<StatInfo>{},), True;
            assertEq("hash<StatInfo>", l[0].fullType());
            assertEq("list<hash<StatInfo>>", l.fullType());
        }
    }

    issue3404() {
        {
            Program p(PO_NEW_STYLE | PO_REQUIRE_TYPES | PO_STRICT_ARGS);
            assertThrows("PARSE-TYPE-ERROR", \p.parse(), ("*hash<string, bool> h0; hash h1; h0 = h1;", ""));
        }

        {
            Program p(PO_NEW_STYLE | PO_REQUIRE_TYPES);
            assertThrows("PARSE-TYPE-ERROR", \p.parse(), ("*hash<string, bool> h0; hash h1; h0 = h1;", ""));
        }

        {
            Program p(PO_NEW_STYLE);
            assertThrows("PARSE-TYPE-ERROR", \p.parse(), ("*hash<string, bool> h0; hash h1; h0 = h1;", ""));
        }

        # make sure complex type assignments work when expected
        {
            hash<string, bool> h1();
            hash<string, bool> h2();
            hash<string, bool> h3 = h1 + h2;
            assertEq("hash<string, bool>", h3.fullType());
        }

        {
            hash<auto> h1 = {"a": True};
            hash<auto> h2 = {"b": True};
            hash<string, bool> h3 = h1 + h2;
            assertEq("hash<string, bool>", h3.fullType());
        }

        {
            list<int> l1();
            list<int> l2();
            list<int> l3 = l1 + l2;
            assertEq("list<int>", l3.fullType());
        }

        {
            list<auto> l1(1);
            list<auto> l2(2);
            list<int> l3 = l1 + l2;
            assertEq("list<int>", l3.fullType());
        }

        {
            hash h;
            *list<string> l = keys h;
            assertEq("nothing", l.fullType());
            l = keys {};
            assertEq("list<string>", l.fullType());
        }
    }

    issue3278() {
        auto x = ();
        {
            hash h = map {$1: True}, x;
            assertEq({}, h);
            assertEq("hash", h.fullType());
        }
        {
            hash<auto> h = map {$1: True}, x;
            assertEq({}, h);
            assertEq("hash<string, bool>", h.fullType());
        }
        {
            hash<string, bool> h = map {$1: True}, x;
            assertEq({}, h);
            assertEq("hash<string, bool>", h.fullType());
        }
        {
            *hash<string, int> x0 = {};
            assertEq("hash<string, int>", x0.fullType());
            hash<string, int> v = x0;
            assertEq({}, v);
            assertEq("hash<string, int>", v.fullType());
        }
        {
            *list<int> x0 = ();
            assertEq("list<int>", x0.fullType());
            list<int> v = x0;
            assertEq((), v);
            assertEq("list<int>", v.fullType());
        }
        {
            *softlist<int> x0 = ();
