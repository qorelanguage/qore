    issue2651() {
        assertEq("hash<string, bool>", (map {$1: True}, keys {}).fullType());
        assertEq("hash<string, bool>", (map {$1: True}, keys {"a": 1}).fullType());
        assertEq("hash<string, bool>", (map {$1: True}, NOTHING.iterator()).fullType());
        assertEq(Type::NothingType, (map {$1: True}, NOTHING).fullType());
        assertEq("list<string>", (keys {}).fullType());
        assertEq("list<bool>", (map True, keys {}).fullType());
        assertEq("list<bool>", (map True, keys {"a": 1}).fullType());
        assertEq("list<bool>", (map True, NOTHING.iterator()).fullType());
        assertEq(Type::NothingType, (map True, NOTHING).fullType());
    }

    issue2647() {
        # test runtime type folding
        {
            auto x = {};
            hash<string, bool> h = x;
            assertEq("hash<auto>", x.fullType());
            assertEq("hash<string, bool>", h.fullType());
        }
        # test parse and runtime type folding
        {
            hash<string, bool> h = {};
            assertEq("hash<string, bool>", h.fullType());
            code test = hash<string, bool> sub () {
                return {};
            };
            assertEq("hash<string, bool>", test().fullType());
        }
        # test runtime type folding
        {
            auto x = ();
            list<bool> l = x;
            assertEq("list<auto>", x.fullType());
            assertEq("list<bool>", l.fullType());
        }
        # test parse and runtime type folding
        {
            list<bool> l = ();
            assertEq("list<bool>", l.fullType());
            code test = list<bool> sub () {
                return ();
            };
            assertEq("list<bool>", test().fullType());
        }
    }

    static hash testAnyArg(auto v) {
        return {
            "type": v.fullType(),
            "complex": v.complexType(),
        };
    }
