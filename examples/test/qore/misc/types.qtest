    issue3157() {
        timeout t = 3s;
        t -= 1s;
        assertEq(2000, t);

        assertEq(NT_DATE, (t - 1s).typeCode());
        assertEq(NT_DATE, (10s - t).typeCode());
        assertEq(NT_DATE, (t + 1s).typeCode());
        assertEq(NT_DATE, (10s + t).typeCode());

        t = 3s;
        t = t - 1s;
        assertEq(2000, t);

        t = 3s;
        t = 1s - t;
        assertEq(-2000, t);

        t = 3s;
        t += 1s;
        assertEq(4000, t);

        t = 3s;
        t = t + 1s;
        assertEq(4000, t);

        t = 3s;
        t = 1s + t;
        assertEq(4000, t);

        t = 3000;
        t -= 1s;
        assertEq(2000, t);

        t = 3000;
        t = t - 1s;
        assertEq(2000, t);

        t = 3000;
        t = 1s - t;
        assertEq(-2000, t);

        t = 3000;
        t += 1s;
        assertEq(4000, t);

        t = 3000;
        t = t + 1s;
        assertEq(4000, t);

        t = 3000;
        t = 1s + t;
        assertEq(4000, t);
    }

