            "name": "test",
            "display_name": "test",
            "scheme": "test",
            "logo": "data",
            "logo_mime_type": MimeTypeSvg,
            "logo_file_name": "test.svg",
        });

        # missing logo
        assertThrows("APP-ERROR", \DataProviderActionCatalog::registerApp(), <DataProviderAppInfo>{
            "name": "test",
            "display_name": "test",
            "desc": "desc",
            "scheme": "test",
            "logo_mime_type": MimeTypeSvg,
            "logo_file_name": "test.svg",
        });

        # missing logo mime type
        assertThrows("APP-ERROR", \DataProviderActionCatalog::registerApp(), <DataProviderAppInfo>{
            "name": "test",
            "display_name": "test",
            "desc": "desc",
            "scheme": "test",
            "logo": "data",
            "logo_file_name": "test.svg",
        });

        # missing scheme
        assertThrows("APP-ERROR", \DataProviderActionCatalog::registerApp(), <DataProviderAppInfo>{
            "name": "test",
            "display_name": "test",
            "desc": "test",
            "logo": "data",
            "logo_mime_type": MimeTypeJpeg,
            "logo_file_name": "test.svg",
        });

        # missing logo file name
        assertThrows("APP-ERROR", \DataProviderActionCatalog::registerApp(), <DataProviderAppInfo>{
            "name": "test",
            "display_name": "test",
            "desc": "desc",
            "scheme": "test",
            "logo": "data",
            "logo_mime_type": MimeTypeJpeg,
        });

        # missing display name
        assertThrows("APP-ERROR", \DataProviderActionCatalog::registerApp(), <DataProviderAppInfo>{
            "name": "test",
            "desc": "desc",
            "scheme": "test",
            "logo": "data",
            "logo_mime_type": MimeTypeJpeg,
            "logo_file_name": "test.svg",
        });

        string app = get_random_string();
        string appdesc = get_random_string();
        string scheme = get_random_string();
        hash<DataProviderAppInfo> my_app = <DataProviderAppInfo>{
            "name": app,
            "display_name": "test",
            "desc": appdesc,
            "scheme": scheme,
            "logo": "data",
            "logo_mime_type": MimeTypeSvg,
            "logo_file_name": "test.svg",
        };
        DataProviderActionCatalog::registerApp(my_app);
        assertEq(my_app, DataProviderActionCatalog::getApp(app));

        assertThrows("APP-ERROR", \DataProviderActionCatalog::getAppEx(), get_random_string());

        # negative tests registering actions
        assertThrows("ACTION-ERROR", \DataProviderActionCatalog::registerAction(), <DataProviderActionInfo>{});

        assertEq(my_app, DataProviderActionCatalog::getAllApps()[0]);

        string act = get_random_string();

        string short_desc = get_random_string(40);
        string desc = get_random_string(100);

        # invalid app
        assertThrows("ACTION-ERROR", \DataProviderActionCatalog::registerAction(), <DataProviderActionInfo>{
            "app": get_random_string(),
            "action": act,
            "path": "/",
            "short_desc": short_desc,
            "desc": desc,
            "action_code": DPAT_API,
        });

        # missing path var definitions
        assertThrows("ACTION-ERROR", \DataProviderActionCatalog::registerAction(), <DataProviderActionInfo>{
            "app": app,
            "action": act,
            "path": "/{key}",
            "short_desc": short_desc,
            "desc": desc,
            "action_code": DPAT_API,
        });

        # extra path var definitions
        assertThrows("ACTION-ERROR", \DataProviderActionCatalog::registerAction(), <DataProviderActionInfo>{
            "app": app,
            "action": act,
            "path": "/",
            "short_desc": short_desc,
            "desc": desc,
            "path_vars": {
                "test": "test",
            },
            "action_code": DPAT_API,
        });

        # invalid action code
        assertThrows("ACTION-ERROR", \DataProviderActionCatalog::registerAction(), <DataProviderActionInfo>{
            "app": app,
            "action": act,
            "path": "/",
            "short_desc": short_desc,
            "desc": desc,
            "action_code": -1,
        });

        hash<DataProviderActionInfo> action = <DataProviderActionInfo>{
            "app": app,
            "action": act,
            "path": "/",
            "short_desc": short_desc,
