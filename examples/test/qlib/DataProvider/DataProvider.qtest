        assertEq(my_app, apps[0]);

        apps = DataProviderActionCatalog::searchAppSubstr(appdesc[5..10], True);
        assertEq(1, apps.size());
        assertEq(my_app, apps[0]);

        apps = DataProviderActionCatalog::searchAppSubstr(appdesc[5..10], False);
        assertEq(1, apps.size());
        assertEq(my_app, apps[0]);

        apps = DataProviderActionCatalog::searchAppRegex(get_random_string());
        assertEq((), apps);

        apps = DataProviderActionCatalog::searchAppSubstr(get_random_string(), True);
        assertEq((), apps);

        apps = DataProviderActionCatalog::searchAppSubstr(get_random_string(), False);
        assertEq((), apps);

        list<hash<DataProviderActionInfo>> actions = DataProviderActionCatalog::getActions(app);
        assertEq(1, actions.size());
        assertEq(action, actions[0]);

        assertEq((), DataProviderActionCatalog::searchAppActionRegex(get_random_string(), short_desc));
        assertEq((), DataProviderActionCatalog::searchAppActionSubstr(get_random_string(), short_desc));

        assertThrows("APP-ERROR", \DataProviderActionCatalog::searchAppActionRegexEx(), (get_random_string(),
            short_desc));
        assertThrows("APP-ERROR", \DataProviderActionCatalog::searchAppActionSubstrEx(), (get_random_string(),
            short_desc));

        assertEq((), DataProviderActionCatalog::searchActionRegex(get_random_string()));
        assertEq((), DataProviderActionCatalog::searchActionSubstr(get_random_string(), True));
        assertEq((), DataProviderActionCatalog::searchActionSubstr(get_random_string(), False));
        assertNothing(DataProviderActionCatalog::getActionsForScheme(get_random_string()));

        actions = DataProviderActionCatalog::searchAppActionRegex(app, short_desc);
        assertEq(1, actions.size());
        assertEq(action, actions[0]);

        actions = DataProviderActionCatalog::searchAppActionSubstr(app, short_desc);
        assertEq(1, actions.size());
        assertEq(action, actions[0]);

        actions = DataProviderActionCatalog::searchActionRegex(short_desc);
        assertEq(1, actions.size());
        assertEq(action, actions[0]);

        actions = DataProviderActionCatalog::searchActionRegex(desc);
        assertEq(1, actions.size());
        assertEq(action, actions[0]);

        actions = DataProviderActionCatalog::searchActionSubstr(short_desc, False);
        assertEq(1, actions.size());
        assertEq(action, actions[0]);

        actions = DataProviderActionCatalog::searchActionSubstr(desc, False);
        assertEq(1, actions.size());
        assertEq(action, actions[0]);

        actions = DataProviderActionCatalog::searchActionSubstr(short_desc, True);
