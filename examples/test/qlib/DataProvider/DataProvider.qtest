        assertEq(my_app, apps[0]);

        apps = DataProviderActionCatalog::searchAppSubstr(app, False);
        assertEq(1, apps.size());
        assertEq(my_app, apps[0]);

        apps = DataProviderActionCatalog::searchAppRegex(appdesc[5..10]);
