        assertEq(action, actions[0]);

        actions = DataProviderActionCatalog::searchActionSubstr(desc, True);
        assertEq(1, actions.size());
        assertEq(action, actions[0]);

        actions = DataProviderActionCatalog::getActionsForScheme(scheme);
        assertEq(1, actions.size());
        assertEq(action, actions[0]);
    }

