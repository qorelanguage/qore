        assertThrows("APP-ERROR", \DataProviderActionCatalog::getActionsEx(), get_random_string());

        assertEq(action, DataProviderActionCatalog::getAllActions()[0]);

        list<hash<DataProviderAppInfo>> apps = DataProviderActionCatalog::searchAppRegex(app);
        assertEq(1, apps.size());
        assertEq(my_app, apps[0]);

        apps = DataProviderActionCatalog::searchAppSubstr(app, True);
