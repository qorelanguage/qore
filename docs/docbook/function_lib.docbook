    </varlistentry>
    <varlistentry>
      <term>Example</term>
      <listitem>
	<programlisting>set_signal_handler(SIGINT, \signal_handler());</programlisting>
      </listitem>
    </varlistentry>
    <varlistentry>
      <term>Restrictions</term>
      <listitem>
	<para>Not available with <link linkend="no-process-control"><code>PO_NO_PROCESS_CONTROL</code></link></para>
      </listitem>
    </varlistentry>
  </variablelist>

