void QoreParserLocation::updatePosition(int f) {
   if (!explicit_first) {
      first_line = f;
      update_parse_line_location(f, f);
   }
   else {
      update_parse_line_location(first_line, f);
      explicit_first = false;
   }
   last_line = f;
}

