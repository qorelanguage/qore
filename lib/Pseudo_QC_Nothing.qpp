    @endcode

    @see <list>::rangeIterator()

    @since %Qore 0.8.8
 */
SingleValueIterator <nothing>::rangeIterator() [flags=CONSTANT] {
   return new QoreObject(QC_SINGLEVALUEITERATOR, 0, new SingleValueIterator(QoreValue()));
}
